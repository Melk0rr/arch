{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "console_title_template": "{{ .Shell }} in {{ .Folder }}",
  "blocks": [
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "properties": {
            "cache_duration": "none",
            "debian": "\uf306",
            "windows": "\uf17a"
          },
          "leading_diamond": "\ue0c7",
          "template": " {{ if .WSL }}WSL at {{ end }}{{.Icon}} ",
          "foreground": "#F0AABF",
          "background": "#2C3040",
          "type": "os",
          "style": "diamond"
        },
        {
          "properties": {
            "cache_duration": "none"
          },
          "trailing_diamond": "\ue0bc",
          "template": " {{if .Root}}{{ .UserName }}üíÄ{{ .HostName }} {{else}}{{ .UserName }}@{{ .HostName }} {{end}} ",
          "foreground": "#F0AABF",
          "background": "#2C3040",
          "type": "session",
          "style": "diamond",
          "foreground_templates": [
            "{{if .Root}}#ff385d{{end}}"
          ]
        },
        {
          "properties": {
            "cache_duration": "none",
            "folder_icon": "\uea7c",
            "folder_separator_icon": " / ",
            "home_icon": "\uf015 ",
            "mapped_locations": {},
            "max_depth": 1,
            "style": "agnoster_short"
          },
          "trailing_diamond": "\ue0b4",
          "template": " {{ .Path }} ",
          "foreground": "#AAB8F0",
          "background": "#6B3A49",
          "type": "path",
          "style": "diamond"
        },
        {
          "properties": {
            "cache_duration": "none",
            "commit_icon": "\uf417 ",
            "fetch_stash_count": true,
            "fetch_status": true,
            "fetch_upstream_icon": true
          },
          "trailing_diamond": "\ue0b4",
          "template": " {{ if( .UpstreamIcon ) }}{{ url .UpstreamIcon .UpstreamURL }}{{ end }}{{ .HEAD }}{{if .BranchStatus }} {{ .BranchStatus }}{{ end }}{{ if .Working.Changed }} \uf044 {{ .Working.String }}{{ end }}{{ if and (.Working.Changed) (.Staging.Changed) }} |{{ end }}{{ if .Staging.Changed }} \uf046 {{ .Staging.String }}{{ end }}{{ if gt .StashCount 0 }} \ueb4b {{ .StashCount }}{{ end }} ",
          "background": "#495C77",
          "type": "git",
          "style": "diamond",
          "foreground_templates": [
            "{{ if or (.Working.Changed) (.Staging.Changed) }}#9AB9E6{{ end }}",
            "{{ if and (gt .Ahead 0) (gt .Behind 0) }}#9AA9E6{{ end }}",
            "{{ if gt .Ahead 0 }}#CCD6FF{{{ end }}",
            "{{ if gt .Behind 0 }}#F0AABF{{ end }}"
          ]
        },
        {
          "properties": {
            "cache_duration": "none"
          },
          "trailing_diamond": "\ue0b4",
          "template": "<transparent> \uf12a</> {{ .Meaning }} ",
          "background": "#910000",
          "type": "exit",
          "style": "diamond"
        }
      ]
    },
    {
      "type": "prompt",
      "alignment": "right",
      "segments": [
        {
          "properties": {
            "always_enabled": true,
            "cache_duration": "none"
          },
          "leading_diamond": "\ue0b6",
          "trailing_diamond": "\ue0bc",
          "template": " \udb86\udd9f {{ .FormattedMs }}",
          "foreground": "#AAC7F0",
          "background": "#0C0D0E",
          "type": "executiontime",
          "style": "diamond",
          "invert_powerline": true
        },
        {
          "properties": {
            "cache_duration": "none"
          },
          "trailing_diamond": "\ue0b4",
          "template": " \udb85\udc4e {{ .CurrentDate | date .Format }} ",
          "foreground": "#AACDF0",
          "background": "#495C77",
          "type": "time",
          "style": "diamond",
          "invert_powerline": true
        }
      ]
    },
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "properties": {
            "always_enabled": true,
            "cache_duration": "none"
          },
          "template": "\n‚ùØ ",
          "foreground": "#fff",
          "type": "exit",
          "style": "plain",
          "foreground_templates": [
            "{{ if gt .Code 0 }}#ff0000{{ end }}"
          ]
        }
      ]
    }
  ],
  "version": 3,
  "final_space": true
}
