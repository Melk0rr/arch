{
    "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
    "version": 2,
    "final_space": true,
    "console_title_template": "{{ .Shell }} in {{ .Folder }}",
    "blocks": [
      {
        "type": "prompt",
        "alignment": "left",
        "segments": [
          {
            "type": "os",
            "style": "diamond",
            "foreground": "#CFAAF0",
            "background": "#2A3F4E",
            "leading_diamond": "\ue0c7",
            "template": " {{ if .WSL }}WSL at {{ end }}{{.Icon}} ",
            "properties": {
              "debian": "\uf306",
              "windows": "\uf17a"
            }
          },
          {
            "type": "session",
            "style": "diamond",
            "foreground_templates": [
              "{{if .Root}}#ff385d{{end}}"
            ],
            "trailing_diamond": "\ue0bc",
            "foreground": "#CFAAF0",
            "background": "#2A3F4E",
            "template": " {{if .Root}}{{ .UserName }}üíÄ{{ .HostName }} {{else}}{{ .UserName }}@{{ .HostName }} {{end}} "
          },
          {
            "type": "path",
            "style": "diamond",
            "trailing_diamond": "\ue0b4",
            "foreground": "#AAD3F0",
            "background": "#543A6B",
            "template": " {{ .Path }} ",
            "properties": {
              "folder_separator_icon": " / ",
              "folder_icon": "\uea7c",
              "home_icon": "\uf015 ",
              "max_depth": 1,
              "mapped_locations": {
              },
              "style": "agnoster_short"
            }
          },
          {
            "type": "git",
            "style": "diamond",
            "trailing_diamond": "\ue0b4",
            "background": "#415582",
            "foreground_templates": [
              "{{ if or (.Working.Changed) (.Staging.Changed) }}#9AB1E6{{ end }}",
              "{{ if and (gt .Ahead 0) (gt .Behind 0) }}#9AC6E6{{ end }}",
              "{{ if gt .Ahead 0 }}#CCEAFF{{{ end }}",
              "{{ if gt .Behind 0 }}#CFAAF0{{ end }}"
            ],
            "template": " {{ if( .UpstreamIcon ) }}{{ url .UpstreamIcon .UpstreamURL }}{{ end }}{{ .HEAD }}{{if .BranchStatus }} {{ .BranchStatus }}{{ end }}{{ if .Working.Changed }} \uf044 {{ .Working.String }}{{ end }}{{ if and (.Working.Changed) (.Staging.Changed) }} |{{ end }}{{ if .Staging.Changed }} \uf046 {{ .Staging.String }}{{ end }}{{ if gt .StashCount 0 }} \ueb4b {{ .StashCount }}{{ end }} ",
            "properties": {
              "fetch_stash_count": true,
              "fetch_status": true,
              "fetch_upstream_icon": true,
              "commit_icon": "\uf417 "
            }
          },
          {
            "type": "exit",
            "style": "diamond",
            "trailing_diamond": "\ue0b4",
            "background": "#910000",
            "template": "<transparent> \uf12a</> {{ .Meaning }} "
          }
        ]
      },
      {
        "type": "prompt",
        "alignment": "right",
        "segments": [
          {
            "type": "executiontime",
            "style": "diamond",
            "foreground": "#AAC0F0",
            "background": "#132025",
            "invert_powerline": true,
            "leading_diamond": "\ue0b6",
            "trailing_diamond": "\ue0bc",
            "template": " Û±¶ü {{ .FormattedMs }}",
            "properties": {
              "always_enabled": true
            }
          },
          {
            "type": "time",
            "style": "diamond",
            "invert_powerline": true,
            "trailing_diamond": "\ue0b4",
            "foreground": "#AADCF0",
            "background": "#415582",
            "template": " Û±ëé {{ .CurrentDate | date .Format }} "
          }
        ]
      },
      {
        "type": "prompt",
        "alignment": "left",
        "segments": [
          {
            "type": "exit",
            "style": "plain",
            "foreground": "#fff",
            "foreground_templates": [
              "{{ if gt .Code 0 }}#ff0000{{ end }}"
            ],
            "template": "\n‚ùØ ",
            "properties": {
              "always_enabled": true
            }
          }
        ]
      }
    ]
  }
